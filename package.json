{
  "name": "vsys",
  "displayName": "VSYS",
  "description": "Quickly manage your system through VS Code",
  "version": "1.0.0",
  "publisher": "SystemTomcat",
  "icon": "vsys.png",
  "repository": {
    "url": "https://github.com/systemtomcat/vscode-vsys"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsys.reboot",
        "title": "VSYS: Reboot this Machine"
      },
      {
        "command": "vsys.shutDown",
        "title": "VSYS: Shut Down this Machine"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "spincycle": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
